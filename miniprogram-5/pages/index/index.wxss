/**index.wxss**/
.usermotto {
  margin-top: 150px;
}
page{
  height:100%;
  width:100%;
  background-color:#faf7ee;
}

.container{
  width: 100%;
  height: 100%;
}

.duration-touch-view {
  width: 100%;
  height: 100%;
}

/*横向布局  */
.layout_horizontal{
  height: 300rpx;
  display: flex;
  /*row 横向  column 列表  */
  flex-direction: row;
}

/*纵向布局  */
.layout_vertical{
  height:550rpx;          /*布局出错改这里*/
  display: flex;
  /*row 横向  column 列表  */
  flex-direction: column;
}

